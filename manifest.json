{
    "name": "silence",
    "description": "mute commentary from websites",
    "version": "0.1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["*://*.youtube.com/*"],
            "js": ["youtube.js"],
            "all_frames": true,
            "run_at": "document_start"
        }
    ]
  }